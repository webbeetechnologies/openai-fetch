{
  "name": "@webbeetechnologies/openai-fetch",
  "version": "2.0.7",
  "description": "OpenAI client powered by fetch",
  "repository": "dexaai/openai-fetch",
  "license": "MIT",
  "author": {
    "name": "Riley Tomasek",
    "email": "hi@rile.yt",
    "url": "https://rile.yt"
  },
  "engines": {
    "node": ">=18"
  },
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "sideEffects": false,
  "files": [
    "dist",
    "openai-types"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "npx esbuild src/index.ts --format=cjs --bundle --outfile=dist/index.js",
    "clean": "rimraf dist openai-types node_modules",
    "clean:build": "rimraf dist openai-types",
    "dev": "tsc --watch",
    "extract-types": "node extract-types.mjs",
    "format": "prettier --check \"**/*.{js,ts,tsx}\"",
    "lint": "eslint src",
    "prebuild": "yarn run clean:build && yarn run extract-types",
    "prepare": "yarn run build",
    "prepublishOnly": "yarn run test",
    "pretest": "yarn run build",
    "release": "np",
    "test": "yarn run lint && yarn run typecheck",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "ky": "^1.1.3"
  },
  "devDependencies": {
    "@dexaai/eslint-config": "^0.4.0",
    "esbuild": "^0.20.2",
    "eslint": "^8.52.0",
    "gts": "^5.3.0",
    "np": "^8.0.4",
    "openai": "^4.12.4",
    "prettier": "^3.0.3",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2"
  },
  "prettier": {
    "singleQuote": true
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@dexaai/eslint-config",
      "@dexaai/eslint-config/node"
    ],
    "ignorePatterns": [
      "dist",
      "node_modules",
      "openai-types"
    ],
    "rules": {
      "no-console": "off",
      "no-process-env": "off"
    }
  }
}
